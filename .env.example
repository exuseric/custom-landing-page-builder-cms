# Database
DATABASE_URI=postgresql://postgres:[password]@[host]/postgres?schema=public

# Supabase Storage (S3-compatible)
SUPABASE_URL=https://[project-id].supabase.co
SUPABASE_SERVICE_KEY=[your-service-role-key]
S3_ENDPOINT=https://[project-id].supabase.co/storage/v1/s3
S3_BUCKET=payload-media
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=[project-id] 
S3_SECRET_ACCESS_KEY=[service-role-key]

# Payload
PAYLOAD_SECRET=[generate-random-string]